<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qss.dao.AuthDao">
    <select id="getUserInfo" parameterType="com.qss.model.LoginFormInfo" resultType="com.qss.model.SysUserInfo">
        select * from sysuserinfo WHERE `NAME`=#{name} and PASSWORD=#{password} and ENABLE=1;
    </select>
</mapper>