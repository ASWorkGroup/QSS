CREATE SCHEMA `QSS` DEFAULT CHARACTER SET utf8 COLLATE utf8_bin ;

USE QSS;

CREATE TABLE `QSS`.`sample` (
  `id` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `sex` VARCHAR(45) NULL,
  PRIMARY KEY (`id`));


/* Table SysUserInfo */

CREATE TABLE `QSS`.`sysuserinfo` (

  `ID` VARCHAR(20) NOT NULL DEFAULT '',

  `NAME` VARCHAR(80) NOT NULL DEFAULT '',

  `ENABLE` INT NOT NULL DEFAULT 0,

  `PASSWORD` VARCHAR(20) NOT NULL DEFAULT '',

  `OMOperatingUnitNumber` VARCHAR(8) NOT NULL DEFAULT '',

  `EMAIL` VARCHAR(80) DEFAULT '',

  `LANGUAGE` VARCHAR(7) NOT NULL DEFAULT '',

  `DEFAULTCOUNTRYREGION` VARCHAR(10) NOT NULL DEFAULT '',

  `PREFERREDTIMEZONE` INT NOT NULL DEFAULT 0,

  `RECVERSION` INT NOT NULL DEFAULT 0,

  `RECID` INT NOT NULL DEFAULT 0,

  PRIMARY KEY (`ID`));

/* Table OMOPeratingUnit */

CREATE TABLE `QSS`.`OMOPeratingUnit` (

  `OMOperatingUnitNumber` VARCHAR(8) NOT NULL DEFAULT '',

  `OMOperatingUnitType` INT NOT NULL DEFAULT 0,
  
  `CURRENCY` VARCHAR(3) NOT NULL DEFAULT '',

  `NAME` VARCHAR(80) NOT NULL DEFAULT '',

  `RECVERSION` INT NOT NULL DEFAULT 0,

  `RECID` INT NOT NULL DEFAULT 0,

  PRIMARY KEY (`OMOperatingUnitNumber`));
  
  /* Table Currency */

CREATE TABLE `QSS`.`Currency` (

  `CurrencyCode` VARCHAR(3) NOT NULL DEFAULT '',

  `Txt` VARCHAR(60) NOT NULL DEFAULT '',

  PRIMARY KEY (`CurrencyCode`));
  
  /* Table CustGroup */

CREATE TABLE `QSS`.`CustGroup` (

  `CustGroup` VARCHAR(10) NOT NULL DEFAULT '',

  `Name` VARCHAR(60) NOT NULL DEFAULT '',

  PRIMARY KEY (`CustGroup`));
