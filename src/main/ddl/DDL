CREATE SCHEMA `QSS` DEFAULT CHARACTER SET utf8 COLLATE utf8_bin ;

CREATE TABLE `QSS`.`sample` (
  `id` INT NOT NULL,
  `name` VARCHAR(45) NULL,
  `sex` VARCHAR(45) NULL,
  PRIMARY KEY (`id`));

/* Table SysUserInfo */

CREATE TABLE `qss`.`sysuserinfo` (

  `ID` VARCHAR(20) NOT NULL DEFAULT '',

  `NAME` VARCHAR(80) NULL DEFAULT '',

  `ENABLE` INT NULL DEFAULT 0,

  `PASSWORD` VARCHAR(20) NULL DEFAULT '',

  `COMPANY` VARCHAR(8) NULL DEFAULT '',

  `EMAIL` VARCHAR(80) NULL DEFAULT '',

  `LANGUAGE` VARCHAR(7) NULL DEFAULT '',

  `DEFAULTCOUNTRYREGION` VARCHAR(10) NULL DEFAULT '',

  `PREFERREDTIMEZONE` INT NULL DEFAULT 0,

  `RECVERSION` INT NULL DEFAULT 0,

  `RECID` INT NULL DEFAULT 0,

  PRIMARY KEY (`ID`));